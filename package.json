{
	"name": "ionic-configuration-viewer",
	"version": "0.0.0-development",
	"description": "Viewer component for configuration data provided by ionic-configuration-service",
	"author": {
		"name": "Markus Wagner",
		"email": "markus.wagner02@web.de"
	},
	"repository": "https://github.com/Ritzlgrmft/ionic-configuration-viewer.git",
	"license": "MIT",
	"private": false,
	"keywords": [
		"ionic",
		"configuration",
		"environizing"
	],
	"scripts": {
		"build": "npm run clean && npm run compile && npm run copy-scss",
		"clean": "rm -f dist/*.d.ts && rm -f dist/*.js && rm -f dist/*.map && rm -f dist/*.json && rm -f dist/*.scss && rm -f src/*.ngfactory.ts && rm -rf aot",
		"commit": "git-cz",
		"compile": "ngc -p tsconfig.aot.json",
		"copy-scss": "cp src/*.scss dist",
		"karma": "karma start karma.conf.js --browsers Chrome",
		"karma-once": "karma start karma.conf.js --browsers PhantomJS --single-run",
		"lint": "tslint --project tsconfig.aot.json --format verbose --type-check",
		"pack": "npm run build && npm pack",
		"report-coverage": "codecov",
		"semantic-release": "semantic-release pre && npm publish && semantic-release post",
		"test": "npm run build && npm run lint && npm run karma-once"
	},
	"files": [
		"dist",
		"src"
	],
	"main": "dist/index.js",
	"typings": "dist/index.d.ts",
	"peerDependencies": {
		"@angular/common": "^4.0.0",
		"@angular/compiler": "^4.0.0",
		"@angular/compiler-cli": "^4.0.0",
		"@angular/core": "^4.0.0",
		"@angular/forms": "^4.0.0",
		"@angular/http": "^4.0.0",
		"@angular/platform-browser": "^4.0.0",
		"@angular/platform-browser-dynamic": "^4.0.0",
		"@angular/platform-server": "^4.0.0",
		"ionic-angular": "^3.0.0",
		"rxjs": "^5.1.1",
		"zone.js": "^0.8.4"
	},
	"dependencies": {
		"ionic-configuration-service": "^5.0.0",
		"ionic-logging-service": "^3.0.0"
	},
	"devDependencies": {
		"@angular/animations": "4.0.0",
		"@angular/common": "4.0.0",
		"@angular/compiler": "4.0.0",
		"@angular/compiler-cli": "4.0.0",
		"@angular/core": "4.0.0",
		"@angular/forms": "4.0.0",
		"@angular/http": "4.0.0",
		"@angular/platform-browser": "4.0.0",
		"@angular/platform-browser-dynamic": "4.0.0",
		"@angular/platform-server": "4.0.0",
		"ionic-angular": "3.0.1",
		"rxjs": "5.1.1",
		"zone.js": "0.8.4",
		"@types/jasmine": "2.5.47",
		"codecov": "2.1.0",
		"codelyzer": "3.0.1",
		"commitizen": "2.9.6",
		"cz-conventional-changelog": "2.0.0",
		"jasmine-core": "2.6.1",
		"karma": "1.6.0",
		"karma-chrome-launcher": "2.0.0",
		"karma-jasmine": "1.1.0",
		"karma-phantomjs-launcher": "1.0.4",
		"karma-typescript": "3.0.1",
		"karma-typescript-es6-transform": "1.0.0",
		"reflect-metadata": "0.1.10",
		"requirejs": "2.3.3",
		"tslint": "5.1.0",
		"typescript": "2.2.1",
		"semantic-release": "6.3.2"
	},
	"config": {
		"commitizen": {
			"path": "cz-conventional-changelog"
		}
	}
}