{
	"name": "ionic-configuration-viewer",
	"version": "0.0.0-development",
	"description": "Viewer component for configuration data provided by ionic-configuration-service",
	"author": {
		"name": "Markus Wagner",
		"email": "markus.wagner02@web.de"
	},
	"repository": "https://github.com/Ritzlgrmft/ionic-configuration-viewer.git",
	"license": "MIT",
	"private": false,
	"keywords": [
		"ionic",
		"configuration",
		"environizing"
	],
	"scripts": {
		"build": "npm run clean && npm run compile && npm run copy-scss",
		"clean": "cd dist && ls | grep -v .npmignore | xargs rm && cd ..",
		"commit": "git-cz",
		"compile": "tsc -p tsconfig.json && rollup -c rollup.config.js dist/configuration-viewer.module.js > dist/configuration-viewer.bundle.js && ngc -p tsconfig.aot.json",
		"copy-scss": "cp src/*.scss dist",
		"karma": "karma start karma.conf.js --browsers Chrome",
		"karma-once": "karma start karma.conf.js --browsers PhantomJS --single-run",
		"lint": "tslint --project tsconfig.aot.json --format verbose",
		"pack": "npm run build && npm pack",
		"report-coverage": "codecov",
		"semantic-release": "semantic-release pre && npm publish && semantic-release post",
		"test": "npm run build && npm run lint && npm run karma-once"
	},
	"files": [
		"dist",
		"src"
	],
	"main": "dist/configuration-viewer.bundle.js",
	"module": "dist/index.js",
	"typings": "dist/index.d.ts",
	"peerDependencies": {
		"@angular/common": "^5.0.0",
		"@angular/compiler": "^5.0.0",
		"@angular/compiler-cli": "^5.0.0",
		"@angular/core": "^5.0.0",
		"@angular/forms": "^5.0.0",
		"@angular/http": "^5.0.0",
		"@angular/platform-browser": "^5.0.0",
		"@angular/platform-browser-dynamic": "^5.0.0",
		"@angular/platform-server": "^5.0.0",
		"ionic-angular": "^3.9.0",
		"rxjs": "^5.5.2",
		"zone.js": "^0.8.18"
	},
	"dependencies": {
		"ionic-configuration-service": "../ionic-configuration-service/ionic-configuration-service-0.0.0-development.tgz",
		"ionic-logging-service": "../ionic-logging-service/ionic-logging-service-0.0.0-development.tgz"
	},
	"devDependencies": {
		"@angular/animations": "5.0.0",
		"@angular/common": "5.0.0",
		"@angular/compiler": "5.0.0",
		"@angular/compiler-cli": "5.0.0",
		"@angular/core": "5.0.0",
		"@angular/forms": "5.0.0",
		"@angular/http": "5.0.0",
		"@angular/platform-browser": "5.0.0",
		"@angular/platform-browser-dynamic": "5.0.0",
		"@angular/platform-server": "5.0.0",
		"ionic-angular": "3.9.2",
		"rxjs": "5.5.2",
		"zone.js": "0.8.18",
		"@types/jasmine": "2.8.2",
		"babel-polyfill": "6.26.0",
		"codecov": "3.0.0",
		"codelyzer": "4.0.1",
		"commitizen": "2.9.6",
		"cz-conventional-changelog": "2.1.0",
		"jasmine-core": "2.8.0",
		"karma": "1.7.1",
		"karma-chrome-launcher": "2.2.0",
		"karma-jasmine": "1.1.0",
		"karma-phantomjs-launcher": "1.0.4",
		"karma-typescript": "3.0.8",
		"karma-typescript-es6-transform": "1.0.2",
		"reflect-metadata": "0.1.10",
		"requirejs": "2.3.3",
		"rollup": "0.51.7",
		"tslint": "5.8.0",
		"typescript": "2.4.2",
		"semantic-release": "8.2.0"
	},
	"config": {
		"commitizen": {
			"path": "cz-conventional-changelog"
		}
	}
}
