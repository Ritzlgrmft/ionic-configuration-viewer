{
	"compilerOptions": {
		"target": "es5",
		"lib": [
			"dom",
			"es2015"
		],
		// "types": [],
		"module": "commonjs",
		"moduleResolution": "node",
		"emitDecoratorMetadata": true,
		"experimentalDecorators": true,
		"noFallthroughCasesInSwitch": true,
		"noImplicitAny": true,
		"noImplicitReturns": true,
		"noUnusedLocals": true,
		"noUnusedParameters": true,
		"declaration": true,
		"sourceMap": false,
		"outDir": "dist"
	},
	"compileOnSave": false,
	"atom": {
		"rewriteTsconfig": false
	}
}